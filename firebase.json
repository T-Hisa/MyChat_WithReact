{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/direct",
        "destination": "/",
        "type": 301
      }, {
        "source": "/direct/*",
        "destination": "/",
        "type": 301
      }, {
        "source": "/groupchat",
        "destination": "/",
        "type": 301
      }, {
        "source": "/groupchat/*",
        "destination": "/",
        "type": 301
      }, {
        "source": "/creategroup",
        "destination": "/",
        "type": 301
      }, {
        "source": "/notification",
        "destination": "/",
        "type": 301
      }, {
        "source": "/signin",
        "destination": "/",
        "type": 301
      }, {
        "source": "/signup",
        "destination": "/",
        "type": 301
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
